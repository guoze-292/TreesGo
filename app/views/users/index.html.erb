<div class="container-fluid">
  <div class="row">
    <div class="col-md-12 col-xs-12 main">
      <h1 class="page-header">Tree Locations</h1>

      <div id="map">


      </div>
      <form>
        <div class="form-group">
          <label for="exampleInputEmail1">Search for location</label>
          <input type="email" class="form-control" id="exampleInputEmail1" data-bind="textInput: keyword" placeholder="Search for location">
        </div>
        <button type="submit" class="btn btn-default pull-right">Search</button>
      </form>
      <p><h2 class="sub-header">Pick Your Tree!</h2></p>

      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Name</th>
              <th>Type</th>
              <th>Location</th>
              <th>Age</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody data-bind="foreach: trees">
            <tr data-bind="click: $root.selectMarker">
              <td><span class="name" data-bind="text: name"></span></td>
              <td><span class="type" data-bind="text: type"></span></td>
              <td><span class="location"data-bind="text: location"></span></td>
              <td><span class="age" data-bind="text: age"></span></td>
              <td><button type="button" class="btn btn-default btn-xs">Choose</button></td>
            </tr>




          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
